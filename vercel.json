{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/api/index.js" },
      { "src": "/convert", "dest": "/api/index.js" },
      { "src": "/listing", "dest": "/api/index.js" }
    ],
      "functions": {
        "api/*": {
          "memory": 1024,
          "maxDuration": 90
        }
      }

  }
  